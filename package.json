{
  "name": "tlock-js",
  "version": "0.1.0",
  "description": "A library to encrypt data that can only be decrypted in the future using drand",
  "source": "src/index.ts",
  "module": "dist/module.js",
  "types": "dist/index.d.ts",
  "targets": {
    "default": {
      "engines": {
        "outputFormat": "global"
      }
    }
  },
  "scripts": {
    "build": "parcel build",
    "build:ci": "npm run build && npm run lint && npm run check && npm run test",
    "check": "tsc --noEmit",
    "clean": "rm -rf ./dist/*",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "jest --verbose ./test/**/*.test.ts"
  },
  "author": "drand.love",
  "license": "(Apache-2.0 OR MIT)",
  "devDependencies": {
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@types/chai": "^4.3.1",
    "@types/chai-string": "^1.4.2",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.0.4",
    "@types/yup": "^0.29.14",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "chai": "^4.3.7",
    "chai-string": "^1.5.0",
    "eslint": "^8.19.0",
    "jest": "^29.3.1",
    "jest-fetch-mock": "^3.0.3",
    "node-fetch": "^3.3.0",
    "parcel": "^2.6.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@noble/bls12-381": "^1.3.0",
    "@noble/hashes": "^1.1.2",
    "@stablelib/chacha20poly1305": "^1.0.1",
    "buffer": "^6.0.3",
    "drand-client": "^1.0.0-pre.7"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">= 16.0.0"
  }
}
